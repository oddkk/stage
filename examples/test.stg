use mod base.*;
use mod base.init.*;

!bind[](
	bind[](
		return[](10),
		(a: int) => return[](a * 5)),
	(a: int) -> Init[Unit]
		=> printIntTwice(a));

printIntTwice := (val: int) -> Init[Unit]
	=> init.bind[](printInt(val),
		(a: Unit) => printInt(val));

!printLn(match Maybe[int].Some(3) {
	Maybe[int].Some(2)  => "two";
	Maybe[int].Some($a) => "something else";
	Maybe[int].None     => "nothing";
});
